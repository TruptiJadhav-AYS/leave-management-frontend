  "info": {
    "title": "Leave Management API",
    "description": "leave management api description",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "securitySchemes": {
      "JWT-auth": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http",
        "name": "JWT",
        "description": "Enter JWT Token",
        "in": "header"
      }
    },
    "schemas": {
      "CreateEmployeeDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "The name of the employee",
            "example": "ABC"
          },
          "email": {
            "type": "string",
            "description": "The email of the employee",
            "example": "abc@gmail.com"
          },
          "mobile_number": {
            "type": "string",
            "description": "The contact number of the employee",
            "example": "7654567823"
          },
          "department_id": {
            "type": "number",
            "description": "The department id of the employee",
            "example": 1
          },
          "role": {
            "type": "string",
            "description": "The role of the employee",
            "example": "employee"
          },
          "dob": {
            "format": "date-time",
            "type": "string",
            "description": "The date of birth of employee",
            "example": "2003-05-08"
          },
          "gender": {
            "type": "string",
            "description": "The gender of the employee (Male/Female/Other)",
            "example": "Female"
          },
          "manager_id": {
            "type": "number",
            "description": "The manager id of the employee",
            "example": 1
          }
        },
